<!-- Refactor -->
<div class="">
<p>Content: <%= generated_content[:content_type] %></p>
<% if generated_content[:source] == 'NewYork' %>
  <% generated_content[:content].each do |article| %>
    <h4><%= article['title'] %> | <%= article['section'] %></h4>
    <h6><%= article['byline'] %></h6>
    <h5><%= article['published_date'].to_date.strftime("%d %B %Y") %></h5>
    <% if article['multimedia'][2] %>
      <%= image_tag(article['multimedia'][2]['url']) %>
      <h5><%= article['multimedia'][2]['caption'] %></h5>
    <% end %>
    <p><%= link_to 'Read More', article['url'] %></p>
  <% end %>
<% end %>
</div>
